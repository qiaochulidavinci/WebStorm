<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="alt f12" />
    <keyboard-shortcut first-keystroke="ctrl back_quote" />
  </action>
  <action id="EditorCloneCaretAbove">
    <keyboard-shortcut first-keystroke="shift ctrl alt k" />
  </action>
  <action id="EditorCloneCaretBelow">
    <keyboard-shortcut first-keystroke="shift ctrl alt m" />
  </action>
  <action id="QuickChangeScheme" />
  <action id="SelectNextOccurrence" />
  <action id="emacsIDEAs.AceJumpWord">
    <keyboard-shortcut first-keystroke="meta l" />
    <keyboard-shortcut first-keystroke="meta i" second-keystroke="meta f" />
    <keyboard-shortcut first-keystroke="alt j" />
  </action>
</keymap>